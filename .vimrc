execute pathogen#infect()

syntax on
set number
set autoindent
set background=dark
set incsearch
set hlsearch
set nocompatible

let g:solarized_termtrans=1
colorscheme solarized
filetype indent plugin on

let mapleader=","
let g:clang_library_path="/usr/lib/llvm-3.6/lib/libclang-3.6.so"

let g:jedi#show_call_signatures=2
